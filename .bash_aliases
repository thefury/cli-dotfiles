
# git
alias ga='git add'
alias gaa='ga .'
alias gc='git commit'
alias gcm='gc -m'
alias gpr='git pull --rebase'
alias gco='git checkout'
alias gb='git branch'
alias gst='git status'
alias gdel='git status | grep deleted | cut -c 15- | xargs git rm'

# bundler
alias bi='bundle install'
alias be='bundle exec'

# dotfiles
alias brc='vim ~/.bashrc; source ~/.bashrc'


